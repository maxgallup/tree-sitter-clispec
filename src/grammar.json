      }
    },
    "int_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+"
      }
    },
    "float_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+\\.\\d+"
      }
    },
    "string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\"(?:[^\\\"\\\\]|\\\\.)*\\\""
      }
    },
    "bool_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "true"
          },
          {
            "type": "STRING",
            "value": "false"
          }
        ]
