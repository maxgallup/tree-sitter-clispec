{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "clispec",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_declaration"
      }
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_declaration"
        }
      ]
    },
    "doc_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "///"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "named_identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Z][a-zA-Z0-9_]*"
      }
    },
    "int_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+"
      }
    },
    "float_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+\\.\\d+"
      }
    },
    "string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\"[a-zA-Z0-9_]*\\\""
      }
    },
    "bool_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "true"
          },
          {
            "type": "STRING",
            "value": "false"
          }
        ]
      }
    },
    "type_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "FIELD",
          "name": "type_id",
          "content": {
            "type": "SYMBOL",
            "name": "named_identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "type_value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "type_alias"
              },
              {
                "type": "SYMBOL",
                "name": "anonymous_enum_expression"
              },
              {
                "type": "SYMBOL",
                "name": "nested_type"
              }
            ]
          }
        }
      ]
    },
    "nested_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type_id",
          "content": {
            "type": "SYMBOL",
            "name": "named_identifier"
          }
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "nested_type"
              },
              {
                "type": "SYMBOL",
                "name": "named_identifier"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "type_alias": {
      "type": "FIELD",
      "name": "type_id",
      "content": {
        "type": "SYMBOL",
        "name": "named_identifier"
      }
    },
    "anonymous_enum_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_enum"
        },
        {
          "type": "SYMBOL",
          "name": "float_enum"
        },
        {
          "type": "SYMBOL",
          "name": "string_enum"
        }
      ]
    },
    "integer_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "int_literal"
              }
            ]
          }
        }
      ]
    },
    "float_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "float_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "float_literal"
              }
            ]
          }
        }
      ]
    },
    "string_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "string_literal"
              }
            ]
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}