{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "clispec",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "declaration"
      }
    },
    "doc_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "///"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "opt_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "arg_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "cmd_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "metadata_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "doc_comment"
        },
        {
          "type": "SYMBOL",
          "name": "line_comment"
        }
      ]
    },
    "metadata_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "meta"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "name:"
            },
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "string_literal"
              }
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "description:"
            },
            {
              "type": "FIELD",
              "name": "description",
              "content": {
                "type": "SYMBOL",
                "name": "string_literal"
              }
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "version:"
            },
            {
              "type": "FIELD",
              "name": "version",
              "content": {
                "type": "SYMBOL",
                "name": "string_literal"
              }
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "named_identifier": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Z][a-zA-Z0-9_]*"
      }
    },
    "int_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+"
      }
    },
    "float_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "-?\\d+\\.\\d+"
      }
    },
    "string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\"(?:[^\\\"\\\\]|\\\\.)*\\\""
      }
    },
    "bool_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "true"
          },
          {
            "type": "STRING",
            "value": "false"
          }
        ]
      }
    },
    "type_expression": {
      "type": "FIELD",
      "name": "expression",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "named_reference"
          },
          {
            "type": "SYMBOL",
            "name": "anonymous_enum_expression"
          },
          {
            "type": "SYMBOL",
            "name": "nested_type"
          }
        ]
      }
    },
    "nested_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "named_reference"
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "named_reference": {
      "type": "FIELD",
      "name": "id",
      "content": {
        "type": "SYMBOL",
        "name": "named_identifier"
      }
    },
    "anonymous_enum_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_enum"
        },
        {
          "type": "SYMBOL",
          "name": "float_enum"
        },
        {
          "type": "SYMBOL",
          "name": "string_enum"
        }
      ]
    },
    "integer_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "int_literal"
              }
            ]
          }
        }
      ]
    },
    "float_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "float_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "float_literal"
              }
            ]
          }
        }
      ]
    },
    "string_enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "string_literal"
              }
            ]
          }
        }
      ]
    },
    "type_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "named_reference"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "requires_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "requires:"
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        }
      ]
    },
    "excludes_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "excludes:"
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        }
      ]
    },
    "opt_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "opt"
        },
        {
          "type": "SYMBOL",
          "name": "named_reference"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string_literal"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "string_literal"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "arg_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "arg"
        },
        {
          "type": "SYMBOL",
          "name": "named_reference"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "start",
                  "content": {
                    "type": "SYMBOL",
                    "name": "int_literal"
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": ".."
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "FIELD",
                  "name": "end",
                  "content": {
                    "type": "SYMBOL",
                    "name": "int_literal"
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "type_expression"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "cmd_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "cmd"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}