{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "clispec",
      "camelcase": "Clispec",
      "title": "Clispec",
      "scope": "source.clispec",
      "file-types": [
        "clispec"
      ],
      "injection-regex": "^clispec$",
      "class-name": "TreeSitterClispec"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Specifications for command line options to provide type safe cli usage",
    "authors": [
      {
        "name": "Max Gallup",
        "email": "maxgallup@pm.me",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-clispec",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": true
  }
}