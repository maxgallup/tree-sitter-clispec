{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "clispec",
      "camelcase": "CliSpec",
      "title": "CliSpec",
      "scope": "source.clispec",
      "file-types": [
        "csp"
      ],
      "injection-regex": "^clispec$",
      "class-name": "TreeSitterClispec"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Tree-sitter grammar definition for CliSpec",
    "authors": [
      {
        "name": "Max Gallup",
        "email": "maxgallup@pm.me"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-clispec"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}